{
  "profiles": {
    "run": {
      "commandName": "Project",
      "commandLineArgs": "run",
      "environmentVariables": {
        "LEAP_TELEMETRY_OPTOUT": "1"
      }
    },
    "run-phase2": {
      "commandName": "Project",
      "commandLineArgs": "run --feature-flags LeapPhase2",
